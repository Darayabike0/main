from flask import Flask, render_template, request, jsonify
import json

app = Flask(__name__)

# ‡πÇ‡∏´‡∏•‡∏î‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏™‡∏¥‡∏ô‡∏Ñ‡πâ‡∏≤
def load_products():
    with open('data/spotweldpro_data.json', 'r', encoding='utf-8') as f:
        return json.load(f)['SpotWeldPro_Products']

@app.route('/')
def home():
    products = load_products()
    return render_template('product.html', products=products)

@app.route('/api/products', methods=['GET'])
def api_products():
    return jsonify(load_products())

@app.route('/register', methods=['POST'])
def register_product():
    data = request.json
    # ‡∏ö‡∏±‡∏ô‡∏ó‡∏∂‡∏Å‡∏Ç‡πâ‡∏≠‡∏°‡∏π‡∏•‡∏•‡∏á JSON ‡∏´‡∏£‡∏∑‡∏≠ CSV
    return jsonify({"status": "success", "message": "‡∏•‡∏á‡∏ó‡∏∞‡πÄ‡∏ö‡∏µ‡∏¢‡∏ô‡πÄ‡∏£‡∏µ‡∏¢‡∏ö‡∏£‡πâ‡∏≠‡∏¢"})
import markdown

@app.route('/docs/<doc_name>')
def show_doc(doc_name):
    with open(f'docs/{doc_name}.md', 'r', encoding='utf-8') as f:
        content = f.read()
    html = markdown.markdown(content)
    return render_template('doc.html', content=html)

# üîå DALAYAR UI/UX Module API

> API ‡∏™‡∏≥‡∏´‡∏£‡∏±‡∏ö‡πÄ‡∏ä‡∏∑‡πà‡∏≠‡∏°‡∏ï‡πà‡∏≠‡πÇ‡∏°‡∏î‡∏π‡∏•‡∏Å‡∏≤‡∏£‡∏≠‡∏≠‡∏Å‡πÅ‡∏ö‡∏ö‡∏™‡πà‡∏ß‡∏ô‡∏´‡∏ô‡πâ‡∏≤ (UI/UX) ‡∏Å‡∏±‡∏ö‡∏£‡∏∞‡∏ö‡∏ö CRM ‡πÅ‡∏•‡∏∞‡πÅ‡∏ö‡πá‡∏Å‡πÄ‡∏≠‡∏ô‡∏î‡πå‡∏Ç‡∏≠‡∏á DALAYAR ‡πÇ‡∏î‡∏¢‡πÄ‡∏ô‡πâ‡∏ô‡∏Ñ‡∏ß‡∏≤‡∏°‡∏ä‡∏±‡∏î‡πÄ‡∏à‡∏ô ‡∏Ñ‡∏ß‡∏≤‡∏°‡πÑ‡∏ß‡πâ‡∏ß‡∏≤‡∏á‡πÉ‡∏à ‡πÅ‡∏•‡∏∞‡∏Ñ‡∏ß‡∏≤‡∏°‡∏™‡∏≠‡∏î‡∏Ñ‡∏•‡πâ‡∏≠‡∏á‡∏Å‡∏±‡∏ö‡πÅ‡∏ö‡∏£‡∏ô‡∏î‡πå

